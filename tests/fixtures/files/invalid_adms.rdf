<?xml version="1.0" encoding="utf-8" ?>
<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:skos="http://www.w3.org/2004/02/skos/core#">

    <skos:ConceptScheme rdf:about="http://joinup.eu/taxonomy/standardizationLevel#">
    </skos:ConceptScheme>

    <skos:Concept rdf:about="http://joinup.eu/taxonomy/standardizationLevel#1">
        <skos:prefLabel xml:lang="en">Level 1</skos:prefLabel>
        <skos:definition xml:lang="en">Standardization of a general wrapper or header to the artifact (meta-data standardization)</skos:definition>
        <skos:inScheme rdf:resource="http://joinup.eu/taxonomy/standardizationLevel#"/>
    </skos:Concept>

    <skos:Concept rdf:about="http://joinup.eu/taxonomy/standardizationLevel#2">
        <skos:prefLabel xml:lang="en">Level 2</skos:prefLabel>
        <skos:definition xml:lang="en">Level 1 plus standardization of external references or interfaces to other artifacts.</skos:definition>
        <skos:inScheme rdf:resource="http://joinup.eu/taxonomy/standardizationLevel#"/>
    </skos:Concept>

    <skos:Concept rdf:about="http://joinup.eu/taxonomy/standardizationLevel#3">
        <skos:prefLabel xml:lang="en">Level 3</skos:prefLabel>
        <skos:definition xml:lang="en">Level 1 plus Level 2 plus whole content standardization (e-g- detailed XML schema reflecting the entire structure of the artifact).</skos:definition>
        <skos:inScheme rdf:resource="http://joinup.eu/taxonomy/standardizationLevel#"/>
    </skos:Concept>

</rdf:RDF>
